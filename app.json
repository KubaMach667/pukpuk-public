{
	"expo": {
		"name": "bookme-app",
		"slug": "bookme-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/pukpuk-icon.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/pukpuk-icon.png",
				"backgroundColor": "#FFFFFF"
			},
			"package": "com.pukpuk.app"
		},
		"web": {
			"favicon": "./assets/images/pukpuk-icon.png"
		}
	}
}
